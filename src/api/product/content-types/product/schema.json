{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Price": {
      "type": "string"
    },
    "size": {
      "type": "json"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Thumbnail": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "orignal_price": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Description": {
      "type": "text"
    },
    "Colors": {
      "type": "json"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "quantity": {
      "type": "string"
    },
    "ShowInHeader": {
      "type": "boolean"
    }
  }
}
